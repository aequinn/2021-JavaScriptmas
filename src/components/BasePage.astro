---
import Header from '../components/Header.astro';

let {title} = Astro.props;
let {dayCss} = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
    <link href={Astro.resolve('../styles/styles.css')} rel="stylesheet"/>
    {dayCss}
    
</head>
<body>
    <Header></Header>
    <slot></slot>
</body>
</html>